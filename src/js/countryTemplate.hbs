<article>
    <h2>{{nameOfCountry}}</h2>
    <p>Capital: {{capital}}</p>
    <p>Population: {{population}}</p>
    <p>Languages:</p>
    <ul>
        {{#each languages}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <p>Flag:</p>
    <img src="{{flagLink}}" alt="Country flag" width="300" height="300">
</article>